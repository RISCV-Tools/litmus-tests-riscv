RISCV MP+fence.rw.rw+si127
{
uint8_t x[256]; (* two cache lines *)
0:x4=x;
0:x1=0x11; 0:x2=0x12;
1:x4=x;
}

P0                                  | P1                                        ;
sb x1,127(x4) (* W x[127]=0x__11 *) | lh x2,127(x4)  (* R x[127..128]=0x1200 *) ;
fence rw,rw                         |                                           ;
sb x2,128(x4) (* W x[128]=0x12__ *) |                                           ;
exists (1:x2=0x1200)
